{
    "name": "doppiogancio/example-project",
    "description": "An example project to show the benefit of Mocked Client",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DoppioGancio\\ExampleProject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoppioGancio\\ExampleProject\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Fabrizio Gargiulo",
            "email": "fabrizio.gargiulo@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.5",
        "jms/serializer": "^3.23",
        "php-di/php-di": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "symfony/var-dumper": "^6.2",
        "php-http/discovery": "^1.15",
        "doppiogancio/mocked-client": "^3.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
